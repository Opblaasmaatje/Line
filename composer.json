{
  "name": "opblaamsaatje/line",
  "type": "project",
  "description": "Discord bot to manage OSRS clans.",
  "keywords": [
    "framework",
    "laravel",
    "discord",
    "bot",
    "runescape",
    "wise-old-man"
  ],
  "license": "MIT",
  "require": {
    "php": "^8.4",
    "brick/json-mapper": "^0.1.1",
    "ianw/quickchart": "^1.3",
    "laracord/framework": "^2.3",
    "spatie/guzzle-rate-limiter-middleware": "^2.1",
    "spatie/php-structure-discoverer": "^2.3",
    "spatie/url": "^2.4"
  },
  "require-dev": {
    "beyondcode/laravel-er-diagram-generator": "^5.0",
    "fakerphp/faker": "^1.23",
    "larastan/larastan": "^3.0",
    "laravel/pint": "^1.15",
    "mockery/mockery": "^1.6",
    "phpstan/phpstan": "^2.1",
    "phpunit/phpunit": "^11.5",
    "spatie/phpunit-snapshot-assertions": "^5.2",
    "symplify/easy-coding-standard": "^12.5"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\": "database/seeders/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true,
    "allow-plugins": {
      "php-http/discovery": true
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
